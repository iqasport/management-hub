{
  "name": "management-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "________________local_dev________________": "",
    "dev": "next dev",
    "start": "next start",
    "docker:up": "docker compose up -d",
    "dev:full": "yarn docker:up && yarn generate & yarn dev",
    "docker:down": "docker compose down",
    "migrate:dev": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "ts-node": "ts-node --compiler-options '{\"module\":\"commonjs\"}'",
    "db:setup": "yarn docker:up && sleep 5 && yarn migrate:dev && yarn db:seed",
    "________________graphql________________": "",
    "generate:nexus": "nodemon --exec 'ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql' --ext 'ts' --watch '*/graphql/**/*.ts'",
    "generate:genql": "nodemon --exec 'genql --schema ./graphql/schema.graphql --output ./graphql/generated/genql'  --watch 'graphql/schema.graphql'",
    "generate": "yarn generate:nexus & yarn generate:genql",
    "________________testing________________": "",
    "test": "jest --watch",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prepare": "husky install",
    "________________deploy________________": "",
    "build": "next build"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@genql/runtime": "^2.6.0",
    "@prisma/client": "3.0.2",
    "@sentry/nextjs": "^6.13.2",
    "@svgr/webpack": "^5.5.0",
    "apollo-server-micro": "2.25.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.24.0",
    "framer-motion": "^4.1.17",
    "graphql": "15.5.2",
    "graphql-scalars": "^1.11.0",
    "micro": "^9.3.4",
    "next": "^11.1.2",
    "nexus": "^1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.24.5",
    "sharp": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@emotion/babel-plugin": "^11.3.0",
    "@genql/cli": "^2.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/gtag.js": "^0.0.7",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-jest": "^27.1.1",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.1",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.1",
    "prisma": "3.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}